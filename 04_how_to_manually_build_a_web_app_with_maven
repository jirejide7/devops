sudo apt install -y maven
mvn --version

ls
git clone git@bitbucket.org:$USERNAME/$PROJECTNAME.git
ls
cd projectname
ls
cat README.md

# Build a sample web app
mvn archetype:generate -DgroupId=com.$GROUP_NAME -DartififactId=$MYNAME -DarchetypeArtifactId=maven-archetype-webapp -DinteractiveMode=false
ls

git status
git add .
git commit -m "1st commit - Created webapp template and uploaded to SCM"
git push

curl localhost:$TOMCAT_PORT/$MYNAME/

# In browser
http://$UBUNTU_EC2_PUBLIC_IP:$TOMCAT_PORT/$PROJECTNAME/
