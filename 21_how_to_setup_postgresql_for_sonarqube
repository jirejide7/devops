clear
systemctl status postgresql
q # if necessary

sudo passwd postgres
$POSTGRESQL_PASSWORD
$POSTGRESQL_PASSWORD

su - postgres
createuser $SONARQUBE_USERNAME
psql
ALTER USER $SONARQUBE_USERNAME WITH ENCRYPTED PASSWORD '$SONARQUBE_PASSWORD';
CREATE DATABASE $SONARQUBE_DATABASE OWNER $SONARQUBE_USERNAME;
GRANT ALL PRIVILEGES ON DATABASE $SONARQUBE_DATABASE TO $SONARQUBE_USERNAME;
\q
exit
