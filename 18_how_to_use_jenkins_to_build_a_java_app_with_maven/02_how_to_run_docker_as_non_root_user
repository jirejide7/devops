docker run hello-world
sudo docker run hello-world

sudo groupadd docker
sudo usermod -aG docker $USER
docker run hello-world

newgrp docker
docker run hello-world

docker ps -a
docker stop $HELLO-WORLD_CONTAINER_NAME_1 $HELLO-WORLD_CONTAINER_NAME_2
docker rm $HELLO-WORLD_CONTAINER_NAME_1 $HELLO-WORLD_CONTAINER_NAME_2
docker ps -a

docker images
docker rmi hello-world
docker images
