systemctl status postgresql
q

sudo passwd postgres
$POSTGRESQL_PASSWORD
$POSTGRESQL_PASSWORD

su - postgres
createuser $SONARQUBE_USERNAME
psql
ALTER USER $SONARQUBE_USER WITH ENCRYPTED password '$SONARQUBE_PASSWORD';
CREATE DATABASE $SONARQUBE_DATABASE OWNER $SONARQUBE_USER;
GRANT ALL PRIVILEGES ON DATABASE $SONARQUBE_DATABASE TO $SONARQUBE_USER;
\q
exit
