# @ManagerNode
cd ~/playbooks
sudo vi installJDKOnTarget.yml
i
---
- hosts: Java_Group

  tasks:
  - name: update APT repositories cache
    become: true
    apt:
      update_cache: yes

  - name: install openJDK
    become: true
    apt:
      name: openjdk-11-jdk

Esc
:wq!
sudo ansible-playbook -e 'ansible_python_interpreter=/usr/bin/python3' installJDKOnTarget.yml

# @TargetNode
java -version
